{
	"name": "@rustyjs/template",
	"version": "0.0.1",
	"description": "template",
	"main": "index.cjs",
	"types": "index.d.ts",
	"module": "index.mjs",
	"author": "@rustyjs",
	"license": "MIT",
	"packageManager": "yarn@3.2.4",
	"scripts": {
		"lint": "eslint tests --ext ts --fix",
		"format": "prettier --write tests/**/*.ts",
		"docs": "typedoc",
		"test": "run-p \"test:**\"",
		"test:node": "jest",
		"test:rust": "cargo test",
		"test:bun": "bun run test:node",
		"fmt": "run-p \"fmt:**\"",
		"fmt:node": "prettier -w .",
		"fmt:rust": "cargo +nightly fmt",
		"update": "yarn upgrade-interactive",
		"build": "napi build --platform --release --js index.cjs",
		"post-build": "gen-esm-wrapper index.cjs index.mjs && yarn fmt"
	},
	"napi": {
		"name": "template",
		"triples": {
			"defaults": true,
			"additional": [
				"x86_64-unknown-linux-musl",
				"aarch64-unknown-linux-gnu",
				"aarch64-apple-darwin",
				"aarch64-linux-android",
				"x86_64-unknown-freebsd",
				"aarch64-unknown-linux-musl",
				"armv7-unknown-linux-gnueabihf"
			]
		}
	},
	"devDependencies": {
		"@jjwesterkamp/typedoc-theme": "^1.2.0",
		"@napi-rs/cli": "^2.12.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8",
		"@types/jest": "^29.2.2",
		"@types/node": "^18.11.9",
		"@types/prettier": "^2",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gen-esm-wrapper": "^1.1.3",
		"jest": "^29.3.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"prettier-plugin-toml": "^0.3.1",
		"ts-jest": "^28.0.8",
		"typedoc": "^0.23.20",
		"typedoc-plugin-mdn-links": "^2.0.0",
		"typescript": "^4.8.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rustyjs/project-template.git"
	},
	"files": [
		"index.cjs",
		"index.mjs",
		"index.d.ts"
	],
	"keywords": [
		"napi",
		"napi-rs",
		"rust",
		"bindings"
	],
	"engines": {
		"node": ">=14",
		"npm": ">=7"
	},
	"bugs": {
		"url": "https://github.com/rustyjs/project-template/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@sapphire/prettier-config"
}
